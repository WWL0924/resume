{
  "name": "my-cv",
  "markdownEntryPoints": {
    "indir": "web/markdown",
    "outdir": "web/src/markdown-built"
  },
  "webEntryPoints": [
    {
      "index": "web/src/index.tsx",
      "html": "web/src/index.html",
      "outdir": "/"
    },
    {
      "index": "web/resume/index.tsx",
      "html": "web/resume/index.html",
      "outdir": "/resume"
    }
  ],
  "apiEntryPoint": "api/src/index.ts",
  "watchFiles": [
    "web/markdown/**/*.md"
  ],
  "copyFiles": [
    {
      "type": "web",
      "from": "web/assets",
      "to": "assets"
    },
    {
      "type": "web",
      "from": "web/resume/404.html",
      "to": "resume/404.html"
    },
    {
      "type": "data",
      "from": "api/resources",
      "to": "resources"
    }
  ]
}